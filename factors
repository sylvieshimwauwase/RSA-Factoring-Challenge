#!/usr/bin/python3

import sys

def factorize(num):
    factors = []
    for i in range(2, num // 2 + 1):
        if num % i == 0:
            factors.append((i, num // i))
    return factors

def factor():
    if len(sys.argv) < 2:
        print("Usage: factors <file>")
        sys.exit(1)

    filename = sys.argv[1]
    with open(filename, "r") as file:
        numbers = [int(line.strip()) for line in file]
    
    for num in numbers:
        factorizations = factorize(num)
        print("{:d}=".format(num), end="")
        if factorizations:
            for factorization in factorizations:
                print("{}*{}".format(factorization[0], factorization[1]))
        
        else:
            print("{}".format(num))

factor()

